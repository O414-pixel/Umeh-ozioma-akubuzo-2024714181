#def  grade _score (score) :
	    if 70 <=  score  <=  100 : 
	           return  'A'
	    elif 60 <= score <= 69 :
	    	   return  'B'
	    elif 50 <= score <= 59 :
	    	   return   'C'
	    elif 45 <= score <= 49 :
	    	    return   'D'
	    elif 40 <= score <= 44 :
	    	     return 'E' 
	    elif 0 <= score <= 39 : 
	             return  'F'
	     else:
	     	    return  ' invalid score'
	     	   
	 #  Examples usage
	 score = int(input("Enter your score(0 -100) :  ")) 
	 grade  = grade _score(score)
    print(f"Your grade is : {grade} ")
